{
  "name": "@civicpress/cli",
  "version": "1.0.0",
  "description": "CivicPress Command Line Interface",
  "type": "module",
  "main": "dist/progressive.js",
  "bin": {
    "civic": "dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "build:progressive": "tsc src/progressive.ts --outDir dist --target es2022 --module esnext --moduleResolution node",
    "dev": "tsc --watch",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "civic",
    "government",
    "cli"
  ],
  "author": "CivicPress Team",
  "license": "MIT",
  "dependencies": {
    "@civicpress/core": "workspace:*",
    "@civicpress/storage": "workspace:*",
    "@types/diff": "8.0.0",
    "cac": "^6.7.14",
    "chalk": "^5.0.0",
    "diff": "8.0.2",
    "form-data": "^4.0.0",
    "inquirer": "^9.0.0",
    "node-fetch": "^3.3.2",
    "tar": "^6.2.1",
    "yaml": "^2.3.4"
  },
  "devDependencies": {
    "@types/glob": "9.0.0",
    "@types/inquirer": "9.0.8",
    "@types/jest": "^29.0.0",
    "@types/node": "^20.19.4",
    "@types/node-fetch": "^2.6.11",
    "@types/tar": "6.1.13",
    "@typescript-eslint/eslint-plugin": "8.36.0",
    "@typescript-eslint/parser": "8.36.0",
    "eslint": "9.30.1",
    "jest": "^29.0.0",
    "typescript": "^5.0.0"
  },
  "packageManager": "pnpm@10.12.4"
}
